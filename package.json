{
  "name": "markdown-abstract",
  "version": "0.1.0",
  "description": "A simple and flexible tool to abstractly generate Markdown",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npx tsc",
    "test": "npm run build && mocha tests/** --reporter spec",
    "prettier": "prettier --no-config --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "npm run prettier && npm run lint:fix",
    "docgen": "typedoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shadowtime2000/markdown-abstract.git"
  },
  "author": "shadowtime2000",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shadowtime2000/markdown-abstract/issues"
  },
  "homepage": "https://github.com/shadowtime2000/markdown-abstract#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "chai": "^4.2.0",
    "eslint": "^7.9.0",
    "mocha": "^8.1.3",
    "prettier": "^2.1.1",
    "typedoc": "^0.19.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {}
}
